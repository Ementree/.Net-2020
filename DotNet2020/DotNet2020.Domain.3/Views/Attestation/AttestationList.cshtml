<p>@Html.ActionLink("Назад", "Attestation", "Attestation")</p>
<table border="1">
    <tr>
        <th>ФИО</th>
        <th>Полученные компетенции</th>
        <th>Дата</th>
        <th>Выгрузить</th>
    </tr>
    @foreach (var element in ViewBag.Attestations)
    {
        <tr><td>@element.WorkerName</td>
            <td>
                @if (element.Competences.Count > 0)
                {
                    foreach (var subElement in element.Competences)
                    {
                        <p>@subElement</p>
                    }
                }
                else
                {
                    <p>Компетенции не были получены!</p>
                }
            </td>
            <td>@element.Date.ToString("d")</td>
            <td>@Html.ActionLink($"Выгрузить", "DownloadAttestation", "Attestation", new {id = element.AttestationId}, null)</td></tr>
    }
</table>